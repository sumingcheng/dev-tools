# Microsoft Auth Config
MICROSOFT_CLIENT_ID=""
MICROSOFT_CLIENT_SECRET=""
MICROSOFT_CALLBACK_URL=""
MICROSOFT_SCOPE=""
MICROSOFT_TENANT=""

# Mailer config
MAILER_SMTP_URL=__MY_SMTP_URL__
MAILER_ADDRESS_FROM=__MY_SMTP_USER__

# Rate Limit Config
RATE_LIMIT_TTL=60 # In seconds
RATE_LIMIT_MAX=100 # Max requests per IP


#-----------------------Frontend Config------------------------------#


# Base URLs
VITE_BASE_URL=https:192.168.33.41
VITE_SHORTCODE_BASE_URL=https:192.168.33.41
VITE_ADMIN_URL=https:192.168.33.41/admin

# Backend URLs
VITE_BACKEND_GQL_URL=https:192.168.33.41/backend/graphql
VITE_BACKEND_WS_URL=wss:192.168.33.41/backend/graphql
