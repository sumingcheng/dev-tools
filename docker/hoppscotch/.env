#-----------------------Backend Config------------------------------#
# Prisma Config
DATABASE_URL=postgresql://hoppscotch:hoppscotch@postgres:5432/hoppscotch

# Auth Tokens Config
JWT_SECRET=secret1233
TOKEN_SALT_COMPLEXITY=30
MAGIC_LINK_TOKEN_VALIDITY=30
REFRESH_TOKEN_VALIDITY=604800000
ACCESS_TOKEN_VALIDITY=604800000
# SESSION_SECRET='add some secret here'
SESSION_SECRET=some_secret

# Hoppscotch App Domain Config
REDIRECT_URL=https:192.168.33.41
WHITELISTED_ORIGINS=https:192.168.33.41,https:192.168.33.41/admin,https:192.168.33.41/backend
VITE_ALLOWED_AUTH_PROVIDERS=EMAIL

# Google Auth Config
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
GOOGLE_CALLBACK_URL=""
GOOGLE_SCOPE=""

# Github Auth Config
GITHUB_CLIENT_ID=""
GITHUB_CLIENT_SECRET=""
